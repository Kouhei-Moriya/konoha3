#!/bin/bash
# generated by kimio

GLOBALSCORE=0
GLOBALMAX=0
unset KONOHA_DEBUG
KID=$(minikonoha -q)
OS=$(uname -s)
VER=$(uname -r)
ARCH=$(uname -m)

TRYPROOF="TRYPROOF"
OUTFILE="ckonoha${KID}-${OS}${VER}-${ARCH}"

if [ -f "${OUTFILE}.txt" ] ; then
	rm -rf "${OUTFILE}.txt"
fi

if [ -f "${OUTFILE}.csv" ] ; then
	rm -rf "${OUTFILE}.csv"
fi

echo "${OUTFILE}" >> "${OUTFILE}.csv"

if [ -f TRYPROOF ] ; then
	rm -rf TRYPROOF
fi

echo -n "./proof.sh" >> TRYPROOF

echo "cleaning .. "
rm */*_tested

function try_proof { # tested file
	if [ -f $1 ] ; then
		if [ "$TRYPROOF" -eq 1 ] ; then
			echo "TODO"	
		else
			echo -n " $1" >> TRYPROOF
		fi
	fi
} 


function run_test { # count max profile options, file
	echo -n "[$1/$2] running test $4 $5 .."
	KONOHA_TESTPROFILE=$3 minikonoha $4 $5 > /dev/null
	result=$?
	#echo $result
	if [ $result -eq 0 ] ; then
		echo "[PASS]"
		PASS=$(($PASS + 1))
	else
		echo "[FAILED]"
		echo "## [FAILED] $4 $5" >> "${OUTFILE}.txt"
		cat -n $5 >> "${OUTFILE}.txt"
		TESTED="${5}.${3}_tested"
		echo "RESULT: ${TESTED}" >> "${OUTFILE}.txt"
		KONOHA_TESTPROFILE=$3 minikonoha $4 $5 >> "${OUTFILE}.txt"
		echo "ExitStatus: ${result}" >> "${OUTFILE}.txt"
		if [ $result -eq 1 ]; then
			try_proof $TESTED
		fi
		if [ $result -eq 78 ] ; then
			echo "diff $5.$3_proof $5.$3_tested" >> "${OUTFILE}.txt"
			diff $5.$3_proof $5.$3_tested >> "${OUTFILE}.txt"
			try_proof $TESTED
		fi
		echo "--" >> "${OUTFILE}.txt"
		echo >> "${OUTFILE}.txt"
		echo >> "${OUTFILE}.txt"
		echo >> "${OUTFILE}.txt"
	fi
}

function record_result { # test, count, max
	GLOBALSCORE=$(($GLOBALSCORE + $2))
	GLOBALMAX=$(($GLOBALMAX + $3))
	if [ $3 -ne 0 ] ; then
		per=$(($2 * 100 / $3))
		echo "(TEST $1) [$2/$3, $per%]"
	fi
	echo "$1, $2, $3" >> "${OUTFILE}.csv"
}

function total {
	less "${OUTFILE}.txt"
	per=$(($GLOBALSCORE * 100 / $GLOBALMAX))
	echo "((TOTAL)) [$GLOBALSCORE/$GLOBALMAX, $per%]"
	echo "MORE INFO:"
	echo " ${OUTFILE}.csv"
	echo " ${OUTFILE}.txt"
}

## 0001_Assertion
PASS=0
run_test 1 8 minimum "-MOutputTest" 0001_Assertion/Assertion_False.k
run_test 2 8 minimum "-MOutputTest" 0001_Assertion/Assertion_TopLevelFalse.k
run_test 3 8 minimum "-MOutputTest" 0001_Assertion/Assertion_TopLevelTrue.k
run_test 4 8 minimum "-MOutputTest" 0001_Assertion/Assertion_True.k
run_test 5 8 cstyle "-MOutputTest -Icstyle" 0001_Assertion/Assertion_False.k
run_test 6 8 cstyle "-MOutputTest -Icstyle" 0001_Assertion/Assertion_TopLevelFalse.k
run_test 7 8 cstyle "-MOutputTest -Icstyle" 0001_Assertion/Assertion_TopLevelTrue.k
run_test 8 8 cstyle "-MOutputTest -Icstyle" 0001_Assertion/Assertion_True.k
record_result 0001_Assertion $PASS 8
